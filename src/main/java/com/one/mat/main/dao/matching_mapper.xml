<select id="matchingList" resultType="map">
    SELECT
    m.member_dongAddr
    ,p.pro_dogName
    ,p.pro_dogAge
    ,p.pro_dogGender
    ,p.pro_dogDesc
    ,bt.breedType 
    ,ct.charType 
		FROM member m
		LEFT JOIN pro p ON m.member_idx = p.member_idx
		LEFT JOIN breed b ON p.pro_idx = b.pro_idx
		LEFT JOIN `char` c ON p.pro_idx = c.pro_idx
		LEFT JOIN breedtype bt ON b.breedtype_code = bt.breedtype_code
		LEFT JOIN chartype ct ON c.chartype_code = ct.chartype_code
		WHERE m.member_idx = 1;
</select>

	

